{% extends 'base.html.twig' %}

{% block title %}Seq{% endblock %}

{% block body %}
    <h1>{{'Seq' | trans}}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>{{'Id' | trans}}</th>
                <td>{{ seq.id }}</td>
            </tr>
            <tr>
                <th>{{'Name' | trans}}</th>
                <td>{{ seq.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('seq_index') }}">{{'back to list' | trans}}</a>

    <a href="{{ path('seq_edit', {'id': seq.id}) }}">{{'edit' | trans}}</a>

    {{ include('seq/_delete_form.html.twig') }}
{% endblock %}
