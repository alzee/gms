{% extends 'base.html.twig' %}

{% block title %}Lossrate{% endblock %}

{% block body %}
    <h1>{{'Lossrate' | trans}}</h1>

    <table class="table table-striped table-hover">
        <tbody>
            <tr>
                <th>{{'Id' | trans}}</th>
                <td>{{ lossrate.id }}</td>
            </tr>
            <tr>
                <th>{{'Name' | trans}}</th>
                <td>{{ lossrate.name }}</td>
            <tr>
                <th>{{'Model' | trans}}</th>
                <td>{{ lossrate.model }}</td>
            </tr>
            <tr>
                <th>{{'Craft' | trans}}</th>
                <td>{{ lossrate.craft }}</td>
            </tr>
            </tr>
            <tr>
                <th>{{'Rate' | trans}}</th>
                <td>{{ lossrate.rate }}</td>
            </tr>
            <tr>
                <th>{{'Note' | trans}}</th>
                <td>{{ lossrate.note }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-primary" href="{{ path('lossrate_index') }}">{{'back to list' | trans}}</a>

    <a class="btn btn-warning" href="{{ path('lossrate_edit', {'id': lossrate.id}) }}">{{'edit' | trans}}</a>

    {{ include('lossrate/_delete_form.html.twig') }}
{% endblock %}
