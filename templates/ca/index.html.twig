{% extends 'base.html.twig' %}

{% block title %}Ca index{% endblock %}

{% block body %}
    <h1>Ca index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ca in cas %}
            <tr>
                <td>{{ ca.id }}</td>
                <td>
                    <a href="{{ path('ca_show', {'id': ca.id}) }}">show</a>
                    <a href="{{ path('ca_edit', {'id': ca.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ca_new') }}">Create new</a>
{% endblock %}
